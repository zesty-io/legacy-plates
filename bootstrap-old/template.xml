          </div>]]>
			</field>							
		</fields>
		
		<view file="/views/homepage.tpl" />
	</homepage>
	
	<blueprints>
		
		<!-- basic page blueprint -->
		<blueprint system="basic-page" deactivated="" />
		
		<!-- blog blueprint -->
		<blueprint system="blog" deactivated="" />
		
		<!-- basic blog blueprint -->
		<blueprint system="blog-basic" deactivated="" />
	</blueprints>
	
	<!-- clippings -->	
	<clippings>
		<clipping name="site_name" name_friendly="Site Name" required="0" datatype="text" sort="1" locked="1">
			<![CDATA[Zesty Bootstrapped]]>
		</clipping>
		<clipping name="logo" name_friendly="Site Logo" required="0" datatype="images" sort="2" locked="1" />
		<clipping name="footer_text" name_friendly="Footer Text" required="0" datatype="text" sort="3" locked="0">
			<![CDATA[Footer text can be updated in Content Clippings]]>
		</clipping>
		<clipping name="google_publisher_id" name_friendly="Google Publisher ID" required="0" datatype="text" sort="4" locked="0" />
	</clippings>


	<!-- head files -->	
	<head>
		<item nodeName="html" isLocked="1" active="1" value="html5" />
		<item type="meta" nodeName="meta" isLocked="0" active="1" sort="1">
			<http-equiv>Content-Type</http-equiv>
			<content>text/html;charset=utf-8</content>
		</item>
		<item type="meta" nodeName="meta" isLocked="0" active="1" sort="1">
			<name>viewport</name>
			<content>width=device-width, initial-scale=1.0</content>
		</item>
		
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="2" 	file_name="bootstrap-reset" 				less_file="/resources/less/bootstrap-reset.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="3" 	file_name="bootstrap-mixins" 				less_file="/resources/less/bootstrap-mixins.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="4" 	file_name="bootstrap-variables" 			less_file="/resources/less/bootstrap-variables.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="5" 	file_name="bootstrap" 						less_file="/resources/less/bootstrap.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="6" 	file_name="bootstrap-customize"				less_file="/resources/less/bootstrap-customize.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="7" 	file_name="bootstrap-responsive-util"		less_file="/resources/less/bootstrap-responsive-util.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="8" 	file_name="bootstrap-responsive-1200"		less_file="/resources/less/bootstrap-responsive-1200.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="9" 	file_name="bootstrap-responsive-768-979"	less_file="/resources/less/bootstrap-responsive-768-979.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="10" file_name="bootstrap-responsive-767"		less_file="/resources/less/bootstrap-responsive-767.less" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="11" file_name="bootstrap-responsive-nav"		less_file="/resources/less/bootstrap-responsive-nav.less" />

		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="12" file_name="ie7.css" ie="ie7" file="/resources/css/ie7.css" />
		<item type="text/css" nodeName="link" isLocked="0" active="1" sort="13" file_name="ie8.css" ie="ie8" file="/resources/css/ie8.css" />
		
		<item type="text/javascript" nodeName="script" isLocked="0" active="1" sort="14" src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"  />
		<item type="text/javascript" nodeName="script" isLocked="0" active="1" sort="15" file_name="bootstrap.js" file="/resources/js/bootstrap.js" />
		<item type="text/javascript" nodeName="script" isLocked="0" active="1" sort="16" file_name="main.js" file="/resources/js/main.js" />
	</head>

